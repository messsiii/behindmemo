{
  "name": "ai-love-letter",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "dev:check": "npm run check && npm run dev",
    "precommit": "npm run fix:all",
    "prepare": "husky install",
    "fix:all": "npm run lint:fix && npm run format && npm run typecheck",
    "fix:imports": "eslint --fix --rule 'unused-imports/no-unused-imports: error' .",
    "fix:style": "prettier --write . && eslint --fix .",
    "fix:types": "tsc --noEmit",
    "stress": "NODE_OPTIONS='--experimental-vm-modules' jest tests/stress/stress.test.ts --detectOpenHandles",
    "db:backup": "node dev-tools/backup-db.js",
    "db:check": "node scripts/verify-db-connection.js",
    "db:migrate:dev": "node dev-tools/safe-migrate.js dev",
    "db:migrate:deploy": "node dev-tools/safe-migrate.js deploy",
    "db:push": "node dev-tools/safe-migrate.js push",
    "test:email": "node dev-tools/test-email.js"
  },
